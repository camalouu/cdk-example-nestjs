{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "projects": {
    "applambda": {
      "type": "application",
      "root": "apps/applambda",
      "entryFile": "main",
      "sourceRoot": "apps/applambda/src",
      "compilerOptions": {
        "tsConfigPath": "apps/applambda/tsconfig.app.json"
      }
    },
    "infrastructure": {
      "type": "application",
      "root": "apps/infrastructure",
      "entryFile": "infrastructure"
    }
  }
}